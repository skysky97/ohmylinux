[
    { "key": "Space e",      "command": "workbench.action.togglesidebarvisibility",
                                "when": "sideBarFocus && activeviewlet =='workbench.view.explorer' && !inputFocus" },
    { "key": "Space o",      "command": "outline.focus",
                                "when": "sideBarFocus && activeviewlet =='workbench.view.explorer' && !inputFocus" },
    { "key": "Space z",      "command": "workbench.action.togglezenMode",
                                "when": "!inputFocus" },
    { "key": "Enter",        "command": "search.action.focussearchlist",
                                "when": "sideBarFocus && activeviewlet == 'workbench.view.search' && inputFocus" },
    { "key": "ctrl+j",       "command": "search.action focussearchlist",
                                "when": "sideBarFocus && activeviewlet == 'workbench.view.search' && inputFocus" },
    { "key": "Escape",       "command": "workbench.action.togglesidebarvisibility",
                                "when": "sideBarFocus && activeviewlet == 'workbench.view.search'" },
    { "key": "ctrl+w",       "command": "workbench.action.focusLastEditorGroup" },
    { "key": "ctrl+j",       "command": "selectNextsuggestion",
                                "when": "suggestwidgetvisible" },
    { "key": "ctrl+k",       "command": "selectPrevsuggestion",
                                "when": "suggestwidgetvisible" },
    { "key": "ctrl+j",       "command": "workbench.action.quickopenselectNext",
                                "when": "inquickopen" },
    { "key": "ctrl+k",       "command": "workbench.action.quickopenselectPrevious",
                                "when": "inquickopen" },
    { "key": "ctrl+j",       "command": "-workbench.action.togglePanel" }
]
